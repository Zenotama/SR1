<TableDescription>
	<Name>Special_Spawn</Name>
	<Type>TableDescription</Type>
	<Display_Name>Special Spawn</Display_Name>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Unique>true</Unique>
		<MaxLength>20</MaxLength>
		</Element>
	<Element>
		<Name>Spawn</Name>
		<Type>Element</Type>
		<Element>
			<Name>Time_of_day_start</Name>
			<Type>Int</Type>
			<Display_Name>Time of day (start)</Display_Name>
			<Description>What hour this spawn should become active, on a 24-hour clock (0 = midnight, 23 = 11pm).</Description>
			<Default>8</Default>
			<MinValue>0</MinValue>
			<MaxValue>24</MaxValue>
			</Element>
		<Element>
			<Name>Time_of_day_end</Name>
			<Type>Int</Type>
			<Display_Name>Time of day (end)</Display_Name>
			<Description>What hour this spawn should stop being active, on a 24-hour clock (0 = midnight, 23 = 11pm).</Description>
			<Default>20</Default>
			<MinValue>0</MinValue>
			<MaxValue>24</MaxValue>
			</Element>
		<Element>
			<Name>Min_spacing</Name>
			<Type>Float</Type>
			<Display_Name>Minimum spacing</Display_Name>
			<Description>Each point in the special spawn must be at least this far from other peds before it will spawn.</Description>
			<Default>1.5</Default>
			<MinValue>0.5</MinValue>
			</Element>
		<Element>
			<Name>Spawn_chance</Name>
			<Type>Float</Type>
			<Display_Name>Spawn chance</Display_Name>
			<Description>Between 0 and 1, how often this special spawn should be triggered.</Description>
			<Default>0.0</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Trigger_distance</Name>
			<Type>Float</Type>
			<Display_Name>Trigger distance</Display_Name>
			<Description>Distance in meters from the player to trigger this special spawn script or persona lines.</Description>
			<Default>10.0</Default>
			<MinValue>1</MinValue>
			<MaxValue>50</MaxValue>
			</Element>
		<Element>
			<Name>Lua_script</Name>
			<Type>String</Type>
			<Display_Name>Lua script</Display_Name>
			<Description>For more complex special spawns, name of a script that can handle inter-NPC</Description>
			<Required>false</Required>
			<MaxLength>63</MaxLength>
			</Element>
		<Element>
			<Name>Outdoor_spawn</Name>
			<Type>Selection</Type>
			<Display_Name>Outdoor spawn</Display_Name>
			<Description>This spawn is outdoors and will not be used if it's raining.</Description>
			<Required>false</Required>
			<Default>true</Default>
			<Choice>true</Choice>
			<Choice>false</Choice>
			</Element>
		<Element>
			<Name>Capture_peds</Name>
			<Type>Selection</Type>
			<Display_Name>Capture peds</Display_Name>
			<Description>If true, this special spawn will attract nearby peds</Description>
			<Required>false</Required>
			<Default>true</Default>
			<Choice>true</Choice>
			<Choice>false</Choice>
			</Element>
		<Element>
			<Name>Request_resources</Name>
			<Type>Selection</Type>
			<Display_Name>Allow resource requests</Display_Name>
			<Required>false</Required>
			<Default>true</Default>
			<Choice>true</Choice>
			<Choice>false</Choice>
			</Element>
		<Element>
			<Name>Mutexed</Name>
			<Type>Selection</Type>
			<Display_Name>Mutually exclusive</Display_Name>
			<Description>If set, then exactly one of the characters in the list will spawn when this special spawn is triggered.</Description>
			<Required>false</Required>
			<Default>false</Default>
			<Choice>false</Choice>
			<Choice>true</Choice>
			</Element>
		<Element>
			<Name>Char_List</Name>
			<Type>List</Type>
			<Display_Name>Character List</Display_Name>
			<Description>All the characters involved in this special spawn.</Description>
			<Element>
				<Name>Character</Name>
				<Type>Element</Type>
				<Description>One of the characters used in this spawn.</Description>
				<Element>
					<Name>Display_name</Name>
					<Type>String</Type>
					<Display_Name>Display Name</Display_Name>
					<Description>Used for the special spawn editor.</Description>
					<MaxLength>32</MaxLength>
					</Element>
				<Element>
					<Name>Anim_Actions</Name>
					<Type>Grid</Type>
					<Display_Name>Animations</Display_Name>
					<Required>false</Required>
					<Element>
						<Name>Action</Name>
						<Type>Reference</Type>
						<Reference>
							<File>anim_actions.xtbl</File>
							<Type>Action.Name</Type>
							</Reference>
						</Element>
					<Max_Children>4</Max_Children>
					<Num_Display_Rows>8</Num_Display_Rows>
					</Element>
				<Element>
					<Name>Anim_states</Name>
					<Type>Grid</Type>
