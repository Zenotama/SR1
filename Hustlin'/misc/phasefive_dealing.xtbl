   <TableDescription>
      <Name>Region</Name>
      <Type>TableDescription</Type>
      <Description>A description of a drug dealing region</Description>
      <Element>
         <Name>Name</Name>
         <Type>String</Type>
         <Description>Name of the drug dealing region</Description>
      </Element>
      <Element>
         <Name>Levels</Name>
         <Type>List</Type>
         <Description>A list of all the levels in the activity</Description>
         <Element>
            <Name>Level</Name>
            <Type>Element</Type>
            <Description>A level description</Description>
            <Element>
               <Name>Goal</Name>
               <Type>Int</Type>
               <Description>Number of junkies you must deal to in order to complete the level</Description>
