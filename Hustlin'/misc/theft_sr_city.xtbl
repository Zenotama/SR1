<TableDescription>
	<Name>Territory</Name>
	<Type>TableDescription</Type>
	<Description>A description of the activity</Description>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>A unique name for the region</Description>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>Cutscene</Name>
		<Type>Reference</Type>
		<Description>The cut scene to play before starting the activity</Description>
		<Required>false</Required>
		<Reference>
			<File>cutscene.xtbl</File>
			<Type>Cutscene.Name</Type>
			</Reference>
		</Element>
	<Element>
		<Name>Start_Location</Name>
		<Type>Reference</Type>
		<Description>Where the activity will be started from</Description>
		<Reference>
			<File>..\levels\SaintsRow\activities\theft_sr_city_navpoints.xtbl</File>
			<Type>Navpoint.Name</Type>
			</Reference>
		</Element>
	<Element>
		<Name>Dropoff_Location</Name>
		<Type>Reference</Type>
		<Description>Place you must return the goods to.</Description>
		<Reference>
			<File>..\levels\SaintsRow\activities\theft_sr_city_navpoints.xtbl</File>
			<Type>Navpoint.Name</Type>
			</Reference>
		</Element>
	<Element>
		<Name>Package_Type</Name>
		<Type>Reference</Type>
		<Reference>
			<File>items_3d.xtbl</File>
			<Type>Item.Name</Type>
			</Reference>
		</Element>
	<Element>
		<Name>Last_Cash</Name>
		<Type>Int</Type>
		<Display_Name>Last Level Cash Award</Display_Name>
		<Description>Cash award for completing the maximum level</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Last_Respect</Name>
		<Type>Int</Type>
		<Display_Name>Last Level Respect Award</Display_Name>
		<Description>Props to be given for completing the last level</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Levels</Name>
		<Type>Grid</Type>
		<Description>All of the levels available in the region.</Description>
		<Element>
			<Name>Level</Name>
			<Type>Element</Type>
			<Description>Description of a level</Description>
			<Element>
				<Name>Num_NPCs</Name>
				<Type>Int</Type>
				<Display_Name>Guards Per Shop</Display_Name>
				<Description>Number of guards spawned at this difficulty.</Description>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>Notoriety_Points</Name>
				<Type>Int</Type>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>Return_Time</Name>
				<Type>Float</Type>
				<Description>Time in seconds the player has to return the package.</Description>
				<Default>300</Default>
				</Element>
			<Element>
				<Name>Cash_Award</Name>
				<Type>Int</Type>
				<Display_Name>Cash Award</Display_Name>
				<Description>Cash to be given for completing the level</Description>
				<Default>0</Default>
				<MinValue>0</MinValue>
				</Element>
			<Element>
				<Name>Props_Award</Name>
				<Type>Int</Type>
				<Display_Name>Props Award</Display_Name>
				<Description>Props to be given for completing the level</Description>
				<Default>0</Default>
				<MinValue>0</MinValue>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Shops</Name>
