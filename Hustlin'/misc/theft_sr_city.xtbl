			<Element>
				<Name>Package_Spawn</Name>
				<Type>Reference</Type>
				<Required>false</Required>
				<Reference>
					<File>..\levels\SaintsRow\activities\theft_sr_city_navpoints.xtbl</File>
					<Type>Navpoint.Name</Type>
