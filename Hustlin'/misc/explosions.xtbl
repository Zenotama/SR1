<TableDescription>
	<Name>Explosion</Name>
	<Type>TableDescription</Type>
	<Description>An explosion.  Wooooo.</Description>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Display_Name>Explosion Name</Display_Name>
		<Description>The name of the explosion.</Description>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>Radius</Name>
		<Type>Float</Type>
		<Display_Name>Blast Radius</Display_Name>
		<Description>Radius of the explosion (in meters), inside which objects take damage.</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>FireRadius</Name>
		<Type>Float</Type>
		<Display_Name>Fire Radius</Display_Name>
		<Description>Radius (in meters) inside which humans are set on fire</Description>
		<Required>false</Required>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>AI_Sound_Radius</Name>
		<Type>Float</Type>
		<Display_Name>AI Sound Radius</Display_Name>
		<Description>Maximum distance (in meters) that NPCs can hear the explosion.</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Damage_Max</Name>
		<Type>Int</Type>
		<Display_Name>Maximum Damage</Display_Name>
		<Description>Maximum amount of damage inflicted by the explosion. (occurs at the center of the explosion)</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Damage_Min</Name>
		<Type>Int</Type>
		<Display_Name>Minimum Damage</Display_Name>
		<Description>Minimum amount of damage inflicted by the explosion. (occurs at the edge of the explosion)</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Damage_Max_Player</Name>
		<Type>Int</Type>
		<Display_Name>Maximum Damange v. Player</Display_Name>
		<Description>Maximum amount of damage inflicted by the explosion against a player. (occurs at the center of the  explosion)</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Damage_Min_player</Name>
		<Type>Int</Type>
		<Display_Name>Minimum Damage v. Player</Display_Name>
		<Description>Minimum amount of damage inflicted by the explosion against a player. (occurs at the edge of the explosion)</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Impulse</Name>
		<Type>Float</Type>
		<Description>Maximum impulse (in kg * m / s) to apply at the center of the explosion. This force linearly decreases to 0 at the edge.</Description>
		<Default>0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Effect</Name>
		<Type>Reference</Type>
		<Description>Effect to play for the explosion</Description>
		<Required>false</Required>
		<Reference>
			<File>effects.xtbl</File>
			<Type>Effect.Name</Type>
			</Reference>
