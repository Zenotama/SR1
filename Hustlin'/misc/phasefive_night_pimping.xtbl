   <TableDescription>
      <Name>Region</Name>
      <Type>TableDescription</Type>
      <Description>A description of a pimping region</Description>
      <Element>
         <Name>Name</Name>
         <Type>String</Type>
         <Description>Name of the pimping region</Description>
      </Element>
      <Element>
         <Name>Levels</Name>
         <Type>List</Type>
         <Description>A list of all the levels in the activity</Description>
         <Element>
            <Name>Level</Name>
            <Type>Element</Type>
            <Description>A level description</Description>
            <Element>
               <Name>Goal</Name>
               <Type>Int</Type>
               <Display_Name>Ho Goal</Display_Name>
               <Description>Number of hos you must deliver to complete the level</Description>
               <Default>0</Default>
               <MinValue>0</MinValue>
               <MaxValue>18</MaxValue>
            </Element>
            <Element>
               <Name>Ho_Count</Name>
               <Type>Int</Type>
               <Display_Name>Ho Count</Display_Name>
               <Description>(CURRENTLY IGNORED) How many hos to spawn</Description>
               <Default>0</Default>
               <MinValue>0</MinValue>
            </Element>
            <Element>
               <Name>Thug_Count</Name>
               <Type>Int</Type>
               <Display_Name>Thug Count</Display_Name>
               <Description>(CURRENTLY IGNORED) How many thugs to spawn</Description>
               <Default>0</Default>
               <MinValue>0</MinValue>
            </Element>
            <Element>
               <Name>Location_Count</Name>
               <Type>Int</Type>
               <Display_Name>Location Count</Display_Name>
               <Description>Number of locations to spawn peeps at</Description>
               <Default>1</Default>
               <MinValue>1</MinValue>
            </Element>
            <Element>
               <Name>Locations</Name>
               <Type>List</Type>
               <Description>A list of the locations used by the level</Description>
               <Element>
                  <Name>Location</Name>
                  <Type>Reference</Type>
                  <Description>A location reference</Description>
                  <Reference>
                     <File>..\levels\PhaseFive\phasefive_pimping.xtbl</File>
                     <Type>Region.Locations.Location.Location_Name</Type>
                  </Reference>
               </Element>
               <Max_Children>6</Max_Children>
               <Element_Name>Location</Element_Name>
            </Element>
            <Element>
               <Name>Props</Name>
               <Type>Int</Type>
               <Display_Name>Props Award</Display_Name>
               <Description>The props award for completing the level</Description>
               <Default>0</Default>
               <MinValue>0</MinValue>
            </Element>
         </Element>
         <Max_Children>8</Max_Children>
      </Element>
      <Element>
         <Name>Locations</Name>
