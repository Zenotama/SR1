<TableDescription>
	<Name>Item</Name>
	<Type>TableDescription</Type>
	<Display_Name>3D Item</Display_Name>
	<Description>A 3D item (AKA: "object_item" in the code) represents an object that is placed in the world somehow. It may have been dropped by a human
(on death, for instance) or placed through scripting or code.</Description>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Display_Name>3D Item Name</Display_Name>
		<Description>Unique name of the 3D item, used internally by the game.</Description>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>DisplayName</Name>
		<Type>Reference</Type>
		<Display_Name>Display Name</Display_Name>
		<Required>false</Required>
		<Reference>
			<File>localized_exports\Item_text.xtbl</File>
			<Type>Items_Identifier.Name</Type>
			</Reference>
		</Element>
	<Element>
		<Name>Item_Flags</Name>
		<Type>Flags</Type>
		<Display_Name>Item Flags</Display_Name>
		<Description>Flags for the item.</Description>
		<Required>false</Required>
		<Flag>spin</Flag>
		<Flag>collide_as_sphere</Flag>
		<Flag>keep</Flag>
		<Flag>use_pickup</Flag>
		<Flag>is_drug</Flag>
		<Flag>collide_as_capsule</Flag>
		<Flag>is_money</Flag>
		<Flag>collide_all</Flag>
		<Flag>no_auto_destroy</Flag>
		<Flag>dont_stick_to_ground</Flag>
		<Flag>is_walker</Flag>
		</Element>
	<Element>
		<Name>Mesh</Name>
		<Type>Filename</Type>
		<Display_Name>Static Mesh</Display_Name>
		<Description>Filename of static mesh to use for the 3D item.</Description>
		<Extension>smeshx</Extension>
		<StartingPath>data\models\Items</StartingPath>
		</Element>
	<Element>
		<Name>Touch_Script</Name>
		<Type>Selection</Type>
		<Display_Name>Touch Script</Display_Name>
		<Description>Name of hard-coded script to initiate when item is "touched".</Description>
		<Required>false</Required>
		<Default>give_item</Default>
		<Choice>give_item</Choice>
		<Choice>give_rand_money</Choice>
		<Choice>give_full_health</Choice>
		<Choice>give_health_percent</Choice>
		<Choice>give_money</Choice>
		<Choice>give_big_ass_chain</Choice>
		<Choice>give_drugs</Choice>
		<Choice>give_collectible_item</Choice>
		<Choice>carry_item</Choice>
		<Choice>give_pimp_hat</Choice>
		<Choice>give_mission_item</Choice>
		<Choice>give_bor_money</Choice>
		<Choice>give_bor_chain</Choice>
		</Element>
	<Element>
		<Name>TouchSound</Name>
		<Type>Element</Type>
		<Display_Name>Touch Sound</Display_Name>
		<Required>false</Required>
		<Element>
			<Name>Touch_Sound</Name>
			<Type>Reference</Type>
			<Display_Name>Foley</Display_Name>
			<Description>Name of sound (in the foley table) to play when item is touched.</Description>
			<Reference>
				<File>foley.xtbl</File>
				<Type>Foley.Name</Type>
				</Reference>
			</Element>
		<Element>
			<Name>Frequency</Name>
			<Type>Int</Type>
			<Display_Name>Frequency (ms)</Display_Name>
			<Description>How often can this sound repeat itself in milliseconds.</Description>
			<Required>false</Required>
			<Default>0</Default>
			</Element>
		</Element>
	<Element>
		<Name>Mass</Name>
		<Type>Float</Type>
		<Description>Object mass - required for projectiles.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Respawn_Delay</Name>
		<Type>Int</Type>
		<Display_Name>Respawn Delay</Display_Name>
		<Description>Time (in milliseconds) between respawns.</Description>
		<Required>false</Required>
		<Default>0</Default>
		</Element>
	<Element>
		<Name>Glow_Type</Name>
		<Type>Selection</Type>
		<Display_Name>Glow Type</Display_Name>
		<Default>None</Default>
		<Choice>None</Choice>
		<Choice>Weapons</Choice>
		<Choice>Cash</Choice>
		<Choice>Drugs</Choice>
		</Element>
	<Element>
		<Name>Render_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Render Scale</Display_Name>
		<Description>A scale factor at which to render this item when it is on the ground.  1.0 is normal size.</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		<MinValue>0.1</MinValue>
		</Element>
	<Element>
		<Name>Scale_Ambient</Name>
		<Type>Selection</Type>
		<Display_Name>Scale Ambient</Display_Name>
		<Default>false</Default>
		<Choice>true</Choice>
		<Choice>false</Choice>
		</Element>
	<Element>
		<Name>Collision_Sound</Name>
		<Type>Element</Type>
		<Display_Name>Collision Sound</Display_Name>
		<Description>A foley sound to play when collisions happen, with optional minimum and maximum speeds where volume is 0.0 and 1.0.</Description>
		<Required>false</Required>
		<Element>
			<Name>Foley_Name</Name>
			<Type>Reference</Type>
			<Display_Name>Collision Foley</Display_Name>
			<Reference>
				<File>foley.xtbl</File>
				<Type>Foley.Name</Type>
				</Reference>
			</Element>
		<Element>
			<Name>Min_Speed</Name>
			<Type>Float</Type>
			<Display_Name>Minimum Speed</Display_Name>
			<Required>false</Required>
			<Default>0.0</Default>
			</Element>
		<Element>
			<Name>Max_Speed</Name>
			<Type>Float</Type>
			<Display_Name>Maximum Speed</Display_Name>
			<Required>false</Required>
			<Default>0.0</Default>
			</Element>
		<Element>
			<Name>Frequency</Name>
			<Type>Int</Type>
			<Display_Name>Frequency (ms)</Display_Name>
			<Description>How often can this sound repeat itself in milliseconds.</Description>
			<Required>false</Required>
			<Default>0</Default>
			</Element>
		</Element>
	<Element>
		<Name>Collectible</Name>
		<Type>Element</Type>
		<Description>Gotta collect them all!</Description>
		<Required>false</Required>
		<Element>
			<Name>Number_In_Set</Name>
			<Type>Int</Type>
			<Display_Name>Number In Set</Display_Name>
			<Description>Number of items required to complete this set.</Description>
			<Default>1</Default>
			<MinValue>1</MinValue>
			</Element>
		</Element>
	<Element>
		<Name>LODs</Name>
		<Type>Grid</Type>
		<Display_Name>Levels of Detail</Display_Name>
		<Required>false</Required>
		<Element>
			<Name>LOD_Distance</Name>
			<Type>Float</Type>
			<Display_Name>LOD Distance</Display_Name>
			<Default>10</Default>
			</Element>
		<Max_Children>4</Max_Children>
		<Num_Display_Rows>4</Num_Display_Rows>
		</Element>
	</TableDescription>

